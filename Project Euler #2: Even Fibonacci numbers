#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int t; 
    scanf("%d",&t);
    for(int a0 = 0; a0 < t; a0++){
        long int n,a=1,b=2,c;
        long int sum=0; 
        scanf("%ld",&n);
        for(int i=a;a<=n;i++)
    {
        if(a%2==0)
        {
            sum=sum+a;
        }
        c=a+b;
        a=b;
        b=c;
    }
    printf("%ld\n",sum);
    }
    return 0;
}
